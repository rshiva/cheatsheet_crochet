html, body{
    width: 100%;
}
.cheatsheet-table, .follow-me{
    text-align: center;
}


table.customTable {
  width: 40%;
  background-color: #FFFFFF;
  border-collapse: collapse;
  border-width: 1px;
  border-color: #0E131C;
  border-style: solid;
  color: #000000;
margin: 0 auto;
}

table.customTable td, table.customTable th {
  border-width: 1px;
  border-color: #0E131C;
  border-style: solid;
  padding: 5px;
}

table.customTable thead {
  background-color: #01B9BF;
}

.cheatsheet-table{
    background: asset-data-url('watermark.png') !important;
    background-repeat: repeat !important;
}
