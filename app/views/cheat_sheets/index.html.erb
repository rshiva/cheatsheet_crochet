<h1> Welcome to Bhamila's CheatSheet HomePage</h1>
<table class="customTable">
  <thead>
    <tr>
      <th>Serial No.</th>
      <th>Title</th>
      <th>View</th>
      <th>Edit</th>
      <th>Download</th>
    </tr>
  </thead>
  <tbody>
    <% @cheatsheets.each_with_index do |cheatsheet, index|%>
      <tr>
        <td><%= index+1 %></td>
        <td><%= link_to  "#{cheatsheet.title}", cheatsheet %></td>
        <td><%= link_to  "View", cheatsheet %></td>
        <td><%= link_to  "Edit", edit_cheat_sheet_path(cheatsheet) %></td>
        <td><%= link_to  "Download", download_path(id: cheatsheet.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<div>
<%= link_to  "Create New CheatSheet", new_cheat_sheet_path %>
</<div>
